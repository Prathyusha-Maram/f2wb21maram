extends layout

block content
  h1= title
  p Welcome to Prathyusha Maram #{title}
  - var image_names = ["unpredictable.jpg","oak.jpg","versace.jpg","delicevole.jpg","merazeur.jpg"]
  - var i =0;
  - while(i<3){
     - var randindex = Math.floor(Math.random()*5)
     - img = "/images/"+image_names[randindex]
     - i++
     img(src=img,  height="300px",width="300px",alt="alt")     
  -}